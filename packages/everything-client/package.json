{
  "name": "everything-client",
  "version": "0.0.5",
  "description": "A library for interacting with the Everything search engine.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "import": "./dist/http.mjs",
      "require": "./dist/http.cjs"
    },
    "./ipc": {
      "types": "./dist/ipc.d.ts",
      "import": "./dist/ipc.mjs",
      "require": "./dist/ipc.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepack": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DemoMacro/everything-client.git"
  },
  "keywords": [
    "everything",
    "search",
    "engine",
    "client",
    "library"
  ],
  "author": {
    "name": "Demo Macro",
    "email": "abc@imst.xyz",
    "url": "https://imst.xyz/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DemoMacro/everything-client/issues"
  },
  "homepage": "https://github.com/DemoMacro/everything-client#readme",
  "devDependencies": {
    "ofetch": "1.4.1"
  },
  "optionalDependencies": {
    "koffi": "2.10.1"
  }
}
